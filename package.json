{
  "name": "chat-react",
  "description": "chat-react",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:dev": "tsc &&  cross-env vite build --mode dev",
    "build:test": "tsc &&  cross-env vite build --mode test",
    "build:prod": "tsc &&  cross-env vite build --mode prod",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "prettier-format": "prettier --config .prettierrc --write src/**/*.ts src/**/*.tsx",
    "prettier-watch": "onchange src/**/*.ts src/**/*.tsx spec/**/*.ts -- prettier --write {{changed}}",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chatui/core": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "sass": "^1.61.0",
    "tailwindcss": "^3.2.6",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-html": "^3.2.0"
  }
}
